    <style type="text/css">
        #btnMenuBook:hover {
            cursor: pointer;
        }

        #bookSection {
            margin-bottom: -60px;
            margin-top: 76px;
        }

        .bookBody {
            color: black;
            background-color: #f8f8f8;
        }

