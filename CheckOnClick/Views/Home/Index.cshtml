<body>
    @if (@TempData["Message"] != null)
    {
        <script type="text/javascript">

            var msg = '@TempData["Message"]';
        //    alert(msg);
            swal("Login Failed", msg, "error");

        </script>

    }

    <!-- BEGAIN PRELOADER -->
    <div id="preloader">
        <div id="status">&nbsp;</div>
    </div>
    <!-- END PRELOADER -->
    <!-- SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#"><i class="fa fa-heartbeat"></i></a>
    <!-- END SCROLL TOP BUTTON -->
    <!--=========== BEGIN HEADER SECTION ================-->
    <header id="header">
        <!-- BEGIN MENU -->
        <div class="menu_area">
            <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
                <div class="container">
                    <div class="navbar-header">
                        <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <!-- LOGO -->
                        <!-- TEXT BASED LOGO -->
                        <a class="navbar-brand" href="@Url.Action("Index","Home")" style="margin-top: -10px"><i class="fa fa-heartbeat"></i><span style="font-size: 20px">COC Treatment made easy</span></a>
                        <!-- IMG BASED LOGO  -->
                        <!--  <a class="navbar-brand" href="index.html"><img src="images/logo.png" alt="logo"></a>   -->
                    </div>
                    <div id="navbar" class="navbar-collapse collapse">
                        <ul id="top-menu" class="nav navbar-nav navbar-right main-nav">
                            <li class="active"><a href="@Url.Action("Index","Home")">Home</a></li>
                            <li><a class="menu" href="#topFeature">Features</a></li>

                            <li><a class="menu" href="#service">Services</a></li>

                            @*<li><a class="menu" href="@Url.Action("BookAppointment","Home")">Book Appointment</a></li>*@
                            <li><a class="menu" href="#whychooseSection">About Us</a></li>
                            <li><a class="menu" href="#footer">Contact</a></li>
                            <li><a class="menu" id="btnMenuBook">Book Appointment</a></li>
                            <li><a class="menu" href="" data-toggle="modal" data-target="#loginModal">Login</a></li>
                        </ul>
                    </div><!--/.nav-collapse -->
                </div>
            </nav>
            <!-- Modal for Login -->
            <div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header bg-info">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                            <h5 class="modal-title" style="font-size: 22px;">Login Here</h5>

                        </div>
                        <div class="modal-body bg-info">

                            @using (Html.BeginForm("Login", "Home", FormMethod.Post, new { @onsubmit = "return validateLogin()" }))
                            {
                                <div class="form-group">
                                    <label for="UserName">User Name/Phone number</label>
                                    @Html.TextBoxFor(a => a.UserName, new { @class = "form-control", @placeholder = "Enter User Name/Phone number" })
                                    <span>@Html.ValidationMessageFor(a => a.UserName)</span>
                                </div>

                                <div class="form-group">
                                    @Html.LabelFor(a => a.Password)
                                    @Html.PasswordFor(a => a.Password, new { @class = "form-control", @placeholder = "Enter Password" })
                                    <span>@Html.ValidationMessageFor(a => a.Password)</span>
                                    <span id="err_login" style="font-size:12px; color:red"></span>
                                </div>
                                <div class="row form-group">
                                    <div class="col-md-6"></div>
                                    <div class="col-md-2"><button type="submit" class="btn btn-primary" id="btnLogin" style="font-size: 15px;">Login</button></div>
                                    <div class="col-md-4"><button type="button" class="btn btn-primary" onclick="openModal()" style="font-size: 15px;">Forgot Password</button></div>

                                </div>
                            }

                        </div>
                    </div>
                </div>
            </div>
            <!-- Modal for Forgot Password -->
            <div class="modal fade" id="forgetpassword" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header bg-info">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                            <h5 class="modal-title" style="font-size: 22px;">Forgot Password</h5>

                        </div>
                        <div class="modal-body bg-info">

                            <form method="post">

                                <div class="form-group">
                                    <label for="Email1">Email ID</label>
                                    @Html.TextBoxFor(a => a.Email1, new { @class = "form-control", @placeholder = "Enter Email ID" })
                                    <span>@Html.ValidationMessageFor(a => a.Email1)</span>
                                    <span id="err_forgetPass" style="font-size:12px; color:red"></span>
                                </div>

                                <div class="row form-group">
                                    @*<div class="col-md-8"><span id="err_changePass" style="font-size:12px"></span></div>*@
                                    <div class="col-md-5"></div>
                                    <div class="col-md-3"><button type="button" class="btn btn-info" id="btnBackLogin" style="font-size: 15px;">&larr; Back to Login</button></div>
                                    <div class="col-md-4"><button type="button" class="btn btn-primary" id="Changepassword" style="font-size: 15px;">Reset Password</button></div>

                                </div>
                            </form>

                        </div>
                    </div>
                </div>
            </div>



            <!-- SignUp modal-->
            <div class="modal fade" id="patSignup" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header bg-info">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                            <h5 class="modal-title">Create a New Account: Patient</h5>

                        </div>
                        <div class="modal-body bg-info">
                            <form id="signUpForm" method="post">
                                <div class="form-group">
                                    <label for="FullName">Full Name</label>
                                    <!--      <input type="text" class="form-control" id="pFullName" placeholder="Full Name"> -->
                                    @Html.TextBoxFor(a => a.FullName, new { @class = "form-control", @placeholder = "Full Name", @pattern = "[A-Za-z ]+", @title = "Invaid format" })
                                    <span id="err_fullname"></span>
                                </div>

                                <div class="row">
                                    <div class="col-sm-3">
                                        <label for="Gender">Gender</label>
                                    </div>
                                    <div class="col-sm-3">
                                        <!--     <input type="radio"> Male   -->
                                        @Html.RadioButtonFor(a => a.Gender, "Male") Male
                                    </div>
                                    <div class="col-sm-3">
                                        <!--     <input type="radio"> Female -->
                                        @Html.RadioButtonFor(a => a.Gender, "Female") Female
                                    </div>
                                    <span id="err_gender"></span>
                                </div>
                                <div class="form-group">
                                    <label for="Email">Email address</label>
                                    <!--   <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">    -->
                                    @Html.TextBoxFor(a => a.Email, new { @class = "form-control", @placeholder = "Enter Email", @type = "email" })
                                    <span id="err_email"></span>
                                </div>
                                <div class="form-group">
                                    <label for="Contact">Contact Number</label>
                                    <!--     <input type="number" class="form-control" id="Contact" aria-describedby="emailHelp" placeholder="Contact Number">   -->
                                    @Html.TextBoxFor(a => a.Contact, new { @class = "form-control", @placeholder = "Enter Contact Number" })
                                    <span id="err_contact"></span>
                                </div>
                                <div class="form-group">
                                    <label for="PaPassword">Password</label>
                                    <!--   <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">  -->
                                    @Html.PasswordFor(a => a.PaPassword, new { @class = "form-control", @placeholder = "Create Password" })
                                    <span id="err_pass"></span>
                                </div>
                                <div class="form-group">
                                    <label for="ConfirmPassword">Confirm Password</label>
                                    <!--  <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Confirm Password">   -->
                                    @Html.PasswordFor(a => a.ConfirmPassword, new { @class = "form-control", @placeholder = "Confirm Password" })
                                    <span id="err_conPass"></span>
                                </div>
                                <button type="button" id="btnSignup" class="btn btn-primary center-block">Sign Up</button>
                            </form>

                        </div>
                    </div>
                </div>
            </div>

        </div>
        <!-- END MENU -->
    </header>
    <!--=========== END HEADER SECTION ================-->
    <!--=========== BOOK APPOINTMENT ==================-->
    <section id="bookSection">
        <div class="bookBody">
            <div class="form-grids row widget-shadow" data-example-id="basic-forms">
                <div class="form-title">
                    <h1 class="text-center">-- BOOK APPOINTMENT -- </h1>
                </div>
                <div class="form-body">
                    <div id="bk-appoint">
                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="selectDate">Select Date</label></div>
                            <div class="col-md-7"><input type="date" id="selectDate" class="form-control" /></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="selectSpec">Select Specialization</label></div>
                            <div class="col-md-7">
                                @Html.DropDownList("Specs", null, "-- Select the Specialization --", new { @class = "form-control", @id = "Doc_Spec_Id", @disabled = "disabled" })
                            </div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="selectDoc">Select Doctor</label></div>
                            <div class="col-md-7">
                                <select id="selectDoc" class="form-control" disabled>
                                    <option>-- Select Doctor --</option>
                                </select>
                                <span id="bookSlot" style="color:blue; font-size:14px"></span>
                            </div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="selectSlot">Select Slot</label></div>
                            <div class="col-md-7">
                                <input type="text" id="selectSlot" placeholder="-- Click to select Slot -- " class="form-control" readonly />
                            </div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="patientDesc">Description</label></div>
                            <div class="col-md-7"><textarea maxlength="130" class="form-control" id="patientDesc" rows="3" placeholder="Describe your problem (max - 130 characters)"></textarea></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"></div>
                            <div class="col-md-7">
                                <span id="err" style="color:red; font-size:12px"></span>
                            </div>
                            <div class="col-md-1"></div>
                        </div>
                    </div>

                    <div id="contd-option">
                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-10" style="font-size:20px"><strong>How would you like to Proceed ?</strong></div>
                            <div class="col-md-1"></div>

                        </div>
                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-10">
                                <input type="radio" name="user" value="guest" id="rad-guest" /> &nbsp;&nbsp;&nbsp;&nbsp; <label for="rad-guest">Continue as Guest</label>
                            </div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-10">
                                <input type="radio" name="user" value="sign" id="rad-sign" />  &nbsp;&nbsp;&nbsp;&nbsp; <label for="rad-sign">Sign up to Continue</label>
                            </div>
                            <div class="col-md-1"></div>
                        </div>


                    </div>

                    <div id="form-guest">
                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-10" style="font-size:20px"><strong>Fill your details to proceed ...</strong></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="Gname">Name</label></div>
                            <div class="col-md-7"><input type="text" id="Gname" class="form-control" placeholder="Enter Name" /></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="Gemail">Email</label></div>
                            <div class="col-md-7"><input type="email" id="Gemail" class="form-control" placeholder="Enter Email" /></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="Gcontact">Contact Number</label></div>
                            <div class="col-md-7"><input type="number" id="Gcontact" class="form-control" placeholder="Enter Contact Number" /></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="Ggender">Gender</label></div>
                            <div class="col-md-2"><input type="radio" name="Ggender" id="GgenderM" value="Male" /> &nbsp;&nbsp;&nbsp; <label for="GgenderM">Male</label></div>
                            <div class="col-md-2"><input type="radio" name="Ggender" id="GgenderF" value="Female" /> &nbsp;&nbsp;&nbsp; <label for="GgenderF">Female</label></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"></div>
                            <div class="col-md-7"><span id="err_guest" style="color:red; font-size:12px;"></span></div>
                            <div class="col-md-1"></div>
                        </div>
                    </div>

                    <div id="form-signin">
                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-10" style="font-size:20px"><strong>Fill your details to create account and proceed ...</strong></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="Uname">Name</label></div>
                            <div class="col-md-7"><input type="text" id="Uname" class="form-control" placeholder="Enter Name" /></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="Uemail">Email</label></div>
                            <div class="col-md-7"><input type="email" id="Uemail" class="form-control" placeholder="Enter Email" /></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="Ucontact">Contact Number</label></div>
                            <div class="col-md-7"><input type="number" id="Ucontact" class="form-control" placeholder="Enter Contact Number" /></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="Ugender">Gender</label></div>
                            <div class="col-md-2"><input type="radio" name="Ugender" id="UgenderM" value="Male" /> &nbsp;&nbsp;&nbsp; <label for="UgenderM">Male</label></div>
                            <div class="col-md-2"><input type="radio" name="Ugender" id="UgenderF" value="Female" /> &nbsp;&nbsp;&nbsp; <label for="UgenderF">Female</label></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="UPasword">Password</label></div>
                            <div class="col-md-7"><input type="password" id="UPassword" class="form-control" placeholder="Create Password" /></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="UCPass">Confirm Password</label></div>
                            <div class="col-md-7"><input type="password" id="UCPass" class="form-control" placeholder="Confirm Password" /></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"></div>
                            <div class="col-md-7"><span id="err_user" style="color:red; font-size:12px;"></span></div>
                            <div class="col-md-1"></div>
                        </div>
                    </div>

                    <div id="form-otp">
                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-10" style="font-size:20px"><strong>Verify here to confirm booking ...</strong></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"><label for="OTP_field">Enter One Time Password</label></div>
                            <div class="col-md-7"><input type="number" id="OTP_field" class="form-control" placeholder="Enter OTP" /></div>
                            <div class="col-md-1"></div>
                        </div>

                        <div class="row">
                            <div class="col-md-1"></div>
                            <div class="col-md-3"></div>
                            <div class="col-md-7"><span id="err_otp" style="color:red; font-size:12px;"></span></div>
                            <div class="col-md-1"></div>
                        </div>
                    </div>


                </div>

                <div class="row form-group" style="padding: 1em 0;">
                    <div class="col-md-1"></div>
                    <div class="col-md-4">
                        <a class="btn btn-info form-control" id="bookClose">&Chi; Close</a>
                        <button class="btn btn-info form-control" type="button" id="backAppoint">&larr; GO BACK</button>
                        <button class="btn btn-info form-control" type="button" id="backMenu">&larr; GO BACK</button>
                    </div>
                    <div class="col-md-4">
                        <button class="btn btn-primary form-control" type="button" id="btnBook">BOOK NOW &rarr;</button>
                        <button class="btn btn-primary form-control" type="button" id="btnProceedGuest">PROCEED &rarr;</button>
                        <button class="btn btn-primary form-control" type="button" id="btnProceedUser">PROCEED &rarr;</button>
                        <button class="btn btn-primary form-control" type="button" id="btnVerifyOTP">VERIFY &rarr;</button>
                        <div class="col-md-1"></div>
                    </div>

                </div>
            </div>
        </div>
    </section>
    <!--=========== BEGIN SLIDER SECTION ================-->
